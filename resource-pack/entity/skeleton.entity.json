                    // "variable.minelp_tail_length = query.has_property('tail_length') ? query.property('minelp:tail_length') : variable.minelp_tail_length;",
                    // "variable.minelp_wing_type = query.has_property('wing_type') ? query.property('minelp:wing_type') : variable.minelp_wing_type;",
                    // "variable.minelp_zebra_mane = query.has_property('zebra_mane') ? query.property('minelp:zebra_mane') : variable.minelp_zebra_mane;",
                    "variable.is_holding_right = query.is_item_equipped('main_hand');",
                    "variable.is_holding_left = query.is_item_equipped('off_hand');",
                    "variable.movement_leg_angle = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 20;",
